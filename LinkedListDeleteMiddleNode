public Node DeleteMiddleNode(Node head)
{
  if(head==null || head.next==null)
    return null;
    
  Node current = head;
  Node slow=head;
  Node fast=head;
  Node prev=null;
  
  while(fast.next!=null)
  {
    prev=slow;
    fast=fast.next.next;
    slow=slow.next;
  }
  
  prev.next=slow.next;
  
  return head;
}
