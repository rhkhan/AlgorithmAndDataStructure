public Node remove_duplicates(head):
{
  if(head == null)
    return head;
  
  Node prev=null;
  Node current_old=null;
  Node current=head;
  Node head=null;
  
  while(current!=null)
  {
   current_old=current;
   while(current.next!=null && current.data==current.next.data)
   {
      current=current.next; 
   }
   if(current_old==current)
   {
     if(head==null)
      head=current_old;
     else
      prev.next=current_old;
    current=current.next;
    prev=current_old;
    prev.next=null;
   }
   else
     current=current.next;
  }
  return head;
}
