def is_palindrome(head):
  count=0
  node=head
  while node:
    count+=1
    node=node.next
  
  prev,node=none,head
  for _ in range(count/2):
    temp_node,node.next=node.next,prev
    prev,node=node,temp_node
    
  if count%2==1:
    node=node.next
   
  while node:
    if node.value!=prev.value
      return false
    node,prev=node.next,prev.next
    
  return true;
